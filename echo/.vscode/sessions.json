{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "default",
  "active": "default",
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "autoExecuteCommands": true,
        "name": "server",
        "commands": [
          "cd server",
          "source .venv/bin/activate",
          "./run.sh"
        ]
      },
      [
        {
          "autoExecuteCommands": true,
          "name": "workers",
          "commands": [
            "cd server",
            "source .venv/bin/activate",
            "./run-worker.sh"
          ]
        },
        {
          "autoExecuteCommands": true,
          "name": "workers-cpu",
          "commands": [
            "cd server",
            "source .venv/bin/activate",
            "./run-worker-cpu.sh"
          ]
        },
        {
          "autoExecuteCommands": true,
          "name": "scheduler",
          "commands": [
            "cd server",
            "source .venv/bin/activate",
            "./run-scheduler.sh"
          ]
        }
      ],
      [
        {
          "autoExecuteCommands": true,
          "name": "admin-dashboard",
          "commands": [
            "cd frontend",
            "pnpm run dev"
          ]
        },
        {
          "autoExecuteCommands": true,
          "name": "participant-portal",
          "commands": [
            "cd frontend",
            "pnpm run participant:dev"
          ]
        }
      ]
    ]
  }
}