{
  "version": "0.2.0",
  "configurations": [    
    {
    "name": "Python Debugger: Tasks",
    "type": "debugpy",
    "request": "launch",
    "module": "dembrane.tasks",
    "cwd": "${workspaceFolder}/server"
  },
    {
      "name": "Python Debugger: Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/server/dembrane/main.py",
      "cwd": "${workspaceFolder}/server",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/server"
      },
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "/root/miniconda3/envs/dembrane/bin/python"
    },
    {
      "name": "Attach to Python Functions",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 9091
      },
      "preLaunchTask": "func: host start"
    }
  ]
}
